{
  "name": "tempmail-cloudflare-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker backend for TempMail service",
  "main": "worker.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "d1:create": "wrangler d1 create tempmail-db",
    "d1:migrate": "wrangler d1 execute tempmail-db --file=./schema.sql",
    "d1:migrate:local": "wrangler d1 execute tempmail-db --local --file=./schema.sql",
    "d1:query": "wrangler d1 execute tempmail-db --command"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "d1",
    "tempmail",
    "serverless"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^3.0.0"
  }
}
